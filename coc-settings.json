  //"coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "typescriptreact", "json", "javascriptreact", "typescript.tsx", "svelte", "css", "scss", "html", "java"],
  "coc.preferences.formatOnSaveFiletypes": ["java"],
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": false,
  "prettier.htmlWhitespaceSensitivity": "ignore",
  "prettier.jsxBracketSameLine": true,
  "prettier.proseWrap": "never",
  "prettier.printWidth": 80,
  "prettier.tabWidth": 2,
  "languageserver": {
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "yaml.schemas": {
    "https://raw.githubusercontent.com/docker/compose/master/compose/config/compose_spec.json": "**/docker-compose*.yml"
  }
}

